[Desktop Entry]
_Name=AppCenter
_Comment=Browse and manage apps
Exec=appcenter %U
Icon=system-software-install
_Keywords=install;uninstall;remove;catalogue;store;apps;updates;software;
Terminal=false
Type=Application
StartupNotify=true
Categories=GNOME;GTK;System;PackageManager;
MimeType=x-scheme-handler/appstream;
Actions=AboutDialog;
X-GNOME-UsesNotifications=true

[Desktop Action AboutDialog]
_Name=About AppCenter
Exec=appcenter --about
